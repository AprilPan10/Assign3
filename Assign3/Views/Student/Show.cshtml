@model Assign3.Models.Student
@{
    ViewBag.Title = "Show";
}


    <a class="navlink" href="/Student/List">Back to Student</a>
    <div id="containershow">
        <h2>@Model.StudentFname @Model.StudentLname</h2>
        <img src="~/Content/images/student.png" />
        <div id="content">
            <p>The student's ID is @Model.StudentId </p>
            <p>The student's student number is: @Model.StudentStudentnumber</p>
            <p>The student's enrol date is: @Model.StudentEnroldate</p>
        </div>
    </div>



