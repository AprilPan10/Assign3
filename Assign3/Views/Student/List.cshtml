@model IEnumerable<Assign3.Models.Student>
@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<p>This page should be showing information about all students</p>

@foreach (var student in Model)
{
    <div><a href="/Student/Show/@student.StudentId">@student.StudentFname @student.StudentLname</a></div>


}

